    "@simplewebauthn/server": "^13.1.1",
    "@types/axios": "^0.14.0",
    "@types/react": "^18.2.0",
    "axios": "^1.6.7",
    "base64url": "^3.0.1",
    "bcrypt": "^5.1.1",
